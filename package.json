{
  "name": "fluently-fetch",
  "version": "0.1.0",
  "description": "A fluent interface to the fetch API, based on Superagent.",
  "engines": {
    "node": ">=6.13",
    "npm": ">=6"
  },
  "author": "austin ce <austin.cawley@gmail.com> (https://austince.me)",
  "main": "./dist/lib/fluently-fetch.js",
  "files": [
    "src",
    "dist",
    "README.md",
    "migration.md",
    "tsconfig.json"
  ],
  "scripts": {
    "prepublishOnly": "npm run test && npm run build",
    "lint": "tslint ./src/**/*.ts ./test/**/*.ts",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "test": "npm run test:node && npm run test:browser",
    "test:node": "nyc mocha",
    "posttest:node": "nyc report --reporter=json",
    "test-server:start": "cross-env PORT=5000 ts-node test/util/node/start-server",
    "test-server:start:bg": "cross-env PORT=5000 ts-node test/util/node/start-server &",
    "test-server:kill": "kill $(lsof -t -i:5000)",
    "pretest:browser": "npm run test-server:start:bg && sleep 2",
    "test:browser": "karma start",
    "posttest:browser": "npm run test-server:kill"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "dist",
      "test"
    ],
    "all": false
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "contributors": [
    "austin ce <austin.cawley@gmail.com> (https://austince.me)",
    "Keith Cirkel <oss@keithcirkel.co.uk> (http://keithcirkel.co.uk)"
  ],
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^7.2.0",
    "@commitlint/config-conventional": "^7.1.2",
    "@types/basic-auth": "^1.1.2",
    "@types/chai": "^4.1.6",
    "@types/chai-as-promised": "^7.1.0",
    "@types/chai-http": "^3.0.5",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/formidable": "^1.0.31",
    "@types/isomorphic-fetch": "0.0.34",
    "@types/karma": "^3.0.0",
    "@types/karma-coverage": "^0.5.34",
    "@types/mocha": "^5.2.5",
    "@types/morgan": "^1.7.35",
    "@types/node": "^6.0.118",
    "@types/sinon": "^5.0.5",
    "@types/sinon-chai": "^3.2.0",
    "basic-auth": "^2.0.1",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.2.0",
    "codecov": "^3.1.0",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "formidable": "^1.2.1",
    "http-status-codes": "^1.3.0",
    "husky": "^1.1.1",
    "karma": "^3.1.3",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-express-server": "^0.1.4",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-typescript": "^3.0.13",
    "mocha": "^5.2.0",
    "morgan": "^1.9.1",
    "nyc": "^13.1.0",
    "rimraf": "^2.6.2",
    "sinon": "^6.3.5",
    "sinon-chai": "^3.2.0",
    "source-map-support": "^0.5.9",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "typedoc": "^0.13.0",
    "typescript": "^3.2.1"
  },
  "dependencies": {
    "@types/form-data": "^2.2.1",
    "form-data": "^2.3.3",
    "isomorphic-fetch": "^2.2.1"
  }
}
