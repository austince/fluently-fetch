{
  "extends": "tslint-config-airbnb",
  "rulesDirectory": [],
  "rules": {
    "max-line-length": {
      "options": [
        120
      ]
    },
    "align": [
      false
    ],
    "semicolon": [
      false,
      "never"
    ],
    "no-parameter-reassignment": [
      false
    ],
    "no-console": {
      "severity": "warn",
      "options": [
        "debug",
        "info",
        "log",
        "time",
        "timeEnd",
        "trace"
      ]
    }
  }
}
