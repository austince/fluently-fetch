{
  "extends": "tslint-config-airbnb",
  "rulesDirectory": [],
  "rules": {
    "max-line-length": {
      "options": [
        120
      ]
    },
    "align": [false],
    "semicolon": [false, "never"],
    "no-console": {
      "severity": "error",
      "options": [
        "debug",
        "info",
        "log",
        "time",
        "timeEnd",
        "trace"
      ]
    }
  }
}
